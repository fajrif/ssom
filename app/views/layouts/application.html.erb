	height:59px;
	position:absolute;
	display:block;
	z-index:100;
}

#rev_slider_2_1 .quart1.tparrows:hover { }

#rev_slider_2_1 .quart1.tparrows:before {
	font-family: "ElegantIcons";
	font-size:16px;
	color: #fff;
	display:block;
	line-height: 59px;
	text-align: center;
	position: relative;
}

#rev_slider_2_1 .quart1.tparrows.tp-leftarrow{
	-webkit-transform: translateY(55%);
	-moz-transform: translateY(55%);
	transform: translateY(55%);
