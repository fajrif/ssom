	left: initial;
}

#rev_slider_2_1 .quart1.tparrows.tp-leftarrow:before {
	content: "\23";
}

#rev_slider_2_1 .quart1.tparrows.tp-rightarrow{
	-webkit-transform: translateY(-55%);
	-moz-transform: translateY(-55%);
	transform: translateY(-55%);
	right: 7px;
	left: initial;
}

#rev_slider_2_1 .quart1.tparrows.tp-rightarrow:before {
	content: "\24";
}

#rev_slider_2_1 .quart1.tparrows.tp-leftarrow:hover:before {
	animation: mkdf-move-prev .5s !important;
}
#rev_slider_2_1 .quart1.tparrows.tp-rightarrow:hover:before {
	animation: mkdf-move-next .5s !important;
}
